{
  "name": "procheff",
  "version": "1.0.0",
  "description": "AI-powered recipe management and cooking assistant",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "deploy": "gcloud run deploy procheff --source . --region=us-west1 --allow-unauthenticated --project=degsan-site",
    "deploy:preview": "gcloud run deploy procheff-preview --source . --region=us-west1 --allow-unauthenticated --project=degsan-site",
    "docker:build": "docker build -t procheff .",
    "docker:run": "docker run -p 3000:3000 procheff",
    "claude": "node scripts/claude-auto.mjs",
    "ai": "node scripts/ai-auto.mjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "lucide-react": "^0.545.0",
    "next": "^14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
